{
    "name": "KeePass Chrome",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Access your KeePass passwords offline",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "app": {
        "background": {
          "scripts": ["scripts/main.js"]
        }
    },
    "permissions": [
        "http://*/",
        "https://*/",
        "ftp://*/",
        "fileSystem"
    ]
}
